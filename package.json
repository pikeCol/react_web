{
  "name": "react-webpack",
  "version": "1.0.0",
  "description": "企加云-React-web 端脚手架",
  "author": "zhigang.fang <zhigang.fang.vip@outlook.com>",
  "main": "main.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development node build.config && cross-env NODE_ENV=development PORT=8080 node build/webpack.dev.config.js",
    "alpha": "cross-env NODE_ENV=alpha node build.config && node_modules/.bin/webpack --report --colors --config ./build/webpack.production.config.js",
    "uat": "cross-env NODE_ENV=uat node build.config && node_modules/.bin/webpack --progress --colors --config ./build/webpack.production.config.js",
    "build": "cross-env NODE_ENV=production node_modules/.bin/webpack --progress --colors --config ./build/webpack.production.config.js",
    "lint": "eslint --ext .js src"
  },
  "license": "ISC",
  "dependencies": {
    "antd": "^3.5.3",
    "axios": "^0.18.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "fs": "^0.0.1-security",
    "less": "^3.0.4",
    "less-loader": "^4.1.0",
    "mobx": "^4.2.1",
    "mobx-react": "^5.1.2",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-router-config": "^1.0.0-beta.4",
    "react-router-dom": "^4.2.2",
    "strip-loader": "^0.1.2",
    "styled-components": "^3.2.6"
  },
  "devDependencies": {
    "autoprefixer": "^8.4.1",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-import": "^1.7.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "classnames": "^2.2.5",
    "clean-webpack-plugin": "^0.1.19",
    "compression-webpack-plugin": "^1.1.11",
    "cross-env": "^5.1.5",
    "css-loader": "^0.28.11",
    "eslint": "^4.19.1",
    "eslint-loader": "^2.0.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^1.1.11",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "history": "^4.7.2",
    "html-webpack-plugin": "^3.2.0",
