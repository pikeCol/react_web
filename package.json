    "webpack-stylish": "^0.1.8"
  },
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "/dist/",
      "yarn.lock"
    ]
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "standard --fix",
      "git add"
    ]
  },
  "pre-commit": [
    "lint"
  ]
}
